function img() {
POSITIONAL=()
while [[ $# -gt 0 ]]
do
  key="$1"
  case $key in
    -d|--display)
    GEOMETRY=$(xrandr | grep -oE "[0-9]*x[0-9]*\+[0-9]*\+[0-9]*" | sed -n $2p 2>/dev/null || echo)
    shift # past argument
    shift # past value
    ;;
    *)    # unknown option
    POSITIONAL+=("$1") # save it in an array for later
    shift # past argument
    ;;
  esac
  done
set -- "${POSITIONAL[@]}" # restore positional parameters
if [ -z $GEOMETRY ]; then
  GEOMETRY=$(xrandr | grep -oE "[0-9]*x[0-9]*\+[0-9]*\+[0-9]*" | sed -n 1p)
fi
echo GEOM = $GEOMETRY	
feh -xd --keep-zoom-vp --scale-down --geometry $GEOMETRY "$@" 
}

function pycalc() {
echo -e "import numpy as np; print($@)" | python3
}
alias pycalc='noglob pycalc'

function revsort() {
paste  <(ls $@ |rev) <(ls $@ ) | sort | cut -f 2
}

function revsorte() {
paste  <(echo $@ | tr " " "\n" | rev) <(echo $@ | tr " " "\n" ) | sort | cut -f 2
}

function rimg() {
img -d 3 $(revsorte $@)
}

function dimg() {
return 0
}
